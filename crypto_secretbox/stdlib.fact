void _fact_memzero(secret mut uint8[] buf) {
  _memzero(ref buf);
}

void _fact_memzero64(secret mut uint64[] buf) {
  _memzero64(ref buf);
}

secret bool _crypto_verify_16(secret uint8[16] x, secret uint8[16] y) {
  for (uint32 i = 0 to 16) {
    if (x[i] != y[i]) {
      return false;
    }
  }
  return true;
}
